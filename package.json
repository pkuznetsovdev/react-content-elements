{
  "name": "react-content-elements",
  "version": "1.1.107",
  "private": false,
  "main-webpack": "./dist/main.js",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "homepage": "https://pkuznetsov.github.io/rce",
  "author": {
    "name": "Pavel Kuznetsov",
    "email": "kuznetsov.pavel@protonmail.com",
    "url": "https://www.linkedin.com/in/pkuznetsovdev/"
  },
  "description": "Web Content factory",
  "license": "MIT",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pkuznetsovdev/rce.git"
  },
  "scripts": {
    "build": "rm -rf dist && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "build:webpack": "webpack --mode production",
    "lint": "eslint --fix \"{src/**/*,*}.{js,ts,jsx,tsx}\"",
    "prettier": "prettier --write \"{src,example}/**/*.{js,ts,jsx,tsx}\"",
    "prepare": "npm run build:webpack",
    "prepublishOnly": "npm run prettier && npm run lint",
    "publish": "npm publish"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.4",
    "@types/react": "^18.0.35",
    "babel-loader": "^9.1.2",
    "css-loader": "^6.7.3",
    "style-loader": "^3.3.2",
    "ts-loader": "^9.4.2",
    "typescript": "^5.0.4",
    "webpack": "^5.79.0"
  }
}
